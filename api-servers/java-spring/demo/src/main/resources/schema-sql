
CREATE TABLE USER (
    ID INT PRIMARY KEY AUTOINCREMENT,
    USERNAME VARCHAR(45),
    PASSWORD VARCHAR(60),
    NAME VARCHAR(25),
    AGE INT
);

CREATE TABLE PRODUCT (
    ID INT PRIMARY KEY AUTOINCREMENT,
    NAME VARCHAR(25)
);

INSERT INTO USER (NAME, AGE) VALUES ('Auto', 7);
INSERT INTO USER (NAME, AGE) VALUES ('A', 7);

INSERT INTO USER (USERNAME, PASSWORD) VALUES ('auto', 'auto');
INSERT INTO USER (USERNAME, PASSWORD) VALUES ('a', 'auto');

INSERT INTO PRODUCT (NAME) VALUES ('Honey');
INSERT INTO PRODUCT (NAME) VALUES ('Almond');
